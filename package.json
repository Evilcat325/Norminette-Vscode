{
  "name": "norminette-42",
  "description": "Norminette decorator for vscode with setting \n forked from alexis-visco.morminette-check ",
  "version": "1.0.7",
  "publisher": "evilcat",
  "license": "MIT",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:c"
  ],
  "icon": "42_logo_black.png",
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "*",
    "typescript": "^2.9.2",
    "vsce": "^1.51.0",
    "vscode": "^1.0.0"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "norminette config",
      "properties": {
        "norminette.command": {
          "type": "string",
          "default": "norminette -R CheckForbiddenSourceHeader",
          "description": "Specifies norminette command example norminette -R CheckForbiddenSourceHeader. \n default to norminette"
        },
        "norminette.fileregex": {
          "type": "string",
          "default": "^ft.*\\.[ch]$",
          "description": "Specifies which c file to run norm \n default to ^ft.*\\.[ch]$"
        }
      }
    }
  },
  "repository": "https://github.com/Evilcat325/Norminette-Vscode"
}
